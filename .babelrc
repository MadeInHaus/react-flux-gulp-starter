{
    "presets": [
        "es2015",
        "react",
        "stage-2"
    ],
    "plugins": [
        "transform-class-properties",
        [ "module-resolver", {
            "root": [ "./src/javascript" ],
            "alias": {
                "actions": "actions",
                "components": "components",
                "config": "config",
                "libs": "libs",
                "stores": "stores",
                "utils": "utils",
                "middleware": "middleware"
            }
        }]
    ]
}
